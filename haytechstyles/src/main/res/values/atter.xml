<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PasswordFieldLayout">
        <attr name="ctl_icon_color" format="color"/>
        <attr name="pfl_box_stroke_color_weak_pass" format="color"/>
        <attr name="pfl_box_stroke_color_good_pass" format="color"/>
        <attr name="pfl_box_stroke_color_stroke_pass" format="color"/>
        <attr name="pfl_error_text_color_weak_pass" format="color"/>
        <attr name="pfl_error_text_color_good_pass" format="color"/>
        <attr name="pfl_error_text_color_stroke_pass" format="color"/>
        <attr name="pfl_boxStrokeErrorColor" format="color"/>
        <attr name="pfl_icon" format="color"/>
        <attr name="pfl_text_good_pass" format="string"/>
        <attr name="pfl_text_weak_pass" format="string"/>
        <attr name="pfl_flag_validation" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="RadioButtonField">
        <attr name="mtc_checked_inner_color" format="color"/>
        <attr name="mtc_checked_outer_color" format="color"/>
        <attr name="mtc_duration" format="integer"/>
        <attr name="mtc_inner_width" format="float"/>
        <attr name="mtc_outer_width" format="float"/>
        <attr name="mtc_type">
            <enum name="STROKE" value="0"/>
            <enum name="FILL_AND_STROKE" value="1"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation"/>
    </declare-styleable>

    <declare-styleable name="EditTextVerify">
        <attr name="etv_text_empty_color" format="color"/>
        <attr name="etv_text_full_color" format="color"/>
        <attr name="etv_text_size" format="float"/>
    </declare-styleable>

<!--    CircleProgressBar-->
    <declare-styleable name="CircleFieldProgressBar">
        <attr name="line_count" format="integer|reference"/>
        <attr name="line_width" format="dimension|reference"/>
        <attr name="progress_start_color" format="color|reference"/>
        <attr name="progress_end_color" format="color|reference"/>
        <attr name="progress_text_color" format="color|reference"/>
        <attr name="progress_text_size" format="dimension|reference"/>
        <attr name="progress_stroke_width" format="dimension|reference"/>
        <attr name="progress_background_color" format="color|reference"/>
        <attr name="progress_start_degree" format="integer|reference"/>
        <attr name="drawBackgroundOutsideProgress" format="boolean|reference"/>

        <attr name="progress_blur_radius" format="dimension|reference"/>
        <attr name="progress_blur_style">
            <enum name="normal" value="0"/>
            <enum name="solid" value="1"/>
            <enum name="outer" value="2"/>
            <enum name="inner" value="3"/>
        </attr>

        <attr name="progress_style">
            <enum name="line" value="0"/>
            <enum name="solid" value="1"/>
            <enum name="solid_line" value="2"/>
        </attr>

        <attr name="progress_shader">
            <enum name="linear" value="0"/>
            <enum name="radial" value="1"/>
            <enum name="sweep" value="2"/>
        </attr>

        <attr name="progress_stroke_cap">
            <enum name="butt" value="0"/>
            <enum name="round" value="1"/>
            <enum name="square" value="2"/>
        </attr>

    </declare-styleable>
<!--    End CircleProgressBar-->

    <declare-styleable name="VerifyFieldEditText">
        <attr name="vfe_text_color" format="color|reference" />
        <attr name="vfe_circle_color" format="color|reference" />
        <attr name="vfe_text_count" format="integer" />
        <attr name="vfe_circle_radius" format="integer"/>
        <attr name="vfe_line_width" format="dimension" />
        <attr name="vfe_text_draw_size" format="integer" />
        <attr name="vfe_font" format="string|reference" />
        <attr name="vfe_line_style" >
            <enum name="CIRCLE" value="0" />
            <enum name="LINEAR" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="UsernameFieldLayout">
        <attr name="ufl_icon" format="reference"/>
        <attr name="ufl_style" format="reference"/>
    </declare-styleable>

    <declare-styleable name="FilterFieldEditText">
        <attr name="cv_sb_layoutDirection">
            <enum name="ltr" value="0" />
            <enum name="rtl" value="1" />
            <enum name="inherit" value="2" />
            <enum name="locale" value="3" />
        </attr>

        <attr name="cv_sb_borderEmptyStateColor" format="color" />
        <attr name="cv_sb_borderEmptyStateSize" format="dimension" />
        <attr name="cv_sb_borderEmptyStateRadius" format="dimension" />
        <attr name="cv_sb_borderEmptyStateDashWith" format="dimension" />
        <attr name="cv_sb_borderEmptyStateDashGap" format="dimension" />

        <attr name="cv_sb_borderTextStateColor" format="color" />
        <attr name="cv_sb_borderTextStateSize" format="dimension" />
        <attr name="cv_sb_borderTextStateRadius" format="dimension" />
        <attr name="cv_sb_borderTextStateDashWith" format="dimension" />
        <attr name="cv_sb_borderTextStateDashGap" format="dimension" />

        <attr name="cv_sb_searchIconVisibility">
            <!-- Visible on screen; the default value. -->
            <enum name="visible" value="0" />
            <!-- Not displayed, but taken into account during layout (space is left for it). -->
            <enum name="invisible" value="1" />
            <!-- Completely hidden, as if the view had not been added. -->
            <enum name="gone" value="8" />
        </attr>
        <attr name="cv_sb_searchIconSource" format="reference" />
        <attr name="cv_sb_searchIconScaleType">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>

        <attr name="cv_sb_clearIconVisibility">
            <!-- Visible on screen; the default value. -->
            <enum name="visible" value="0" />
            <!-- Not displayed, but taken into account during layout (space is left for it). -->
            <enum name="invisible" value="1" />
            <!-- Completely hidden, as if the view had not been added. -->
            <enum name="gone" value="8" />
        </attr>
        <attr name="cv_sb_clearIconSource" format="reference" />
        <attr name="cv_sb_clearIconScaleType">
            <enum name="matrix" value="0" />
            <enum name="fitXY" value="1" />
            <enum name="fitStart" value="2" />
            <enum name="fitCenter" value="3" />
            <enum name="fitEnd" value="4" />
            <enum name="center" value="5" />
            <enum name="centerCrop" value="6" />
            <enum name="centerInside" value="7" />
        </attr>
        <attr name="cv_sb_text" format="string" />
        <attr name="cv_sb_hint" format="string" />
        <attr name="cv_sb_textBold" format="boolean" />
        <attr name="cv_sb_textColor" format="color" />
        <attr name="cv_sb_font" format="reference" />
        <attr name="cv_sb_textSize" format="dimension" />
        <attr name="cv_sv_textPaddingLeft" format="dimension" />
        <attr name="cv_sv_textPaddingRight" format="dimension" />
        <attr name="android:ems" />

    </declare-styleable>

</resources>